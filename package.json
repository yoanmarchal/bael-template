{
  "name": "bael-cms-template",
  "version": "0.1.2",
  "description": "A fashionable brutalist blog template using Netlify CMS and Nuxt.js",
  "author": "jake101 <hello@jake101.com>",
  "scripts": {
    "dev": "nuxt",
    "serve": "nuxt",
    "build": "nuxt build",
    "build:api": "netlify-lambda build src/sendgrid",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "postinstall": "netlify-lambda install"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.5.4",
    "@nuxtjs/markdownit": "1.2.6",
    "@nuxtjs/pwa": "3.0.0-beta.16",
    "@sendgrid/client": "6.4.0",
    "bf-solid": "2.11.1",
    "fuse.js": "3.4.5",
    "globby": "^10.0.1",
    "hoek": "6.1.3",
    "netlify-lambda": "^1.6.0",
    "node-sass": "^4.12.0",
    "nuxt": "2.8.1",
    "nuxt-webfontloader": "^1.1.0",
    "sass-loader": "7.2.0",
    "tar": "^4.4.10",
    "vue-moment": "4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-vue": "5.2.3",
    "glob": "7.1.4",
    "prettier": "1.18.2"
  },
  "engines": {
    "node": "10.16.2"
  }
}
