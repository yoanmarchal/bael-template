    "@nuxtjs/pwa": "3.0.0-beta.10",
    "@sendgrid/client": "6.3.0",
    "bf-solid": "2.10.9",
    "fuse.js": "3.4.2",
    "hoek": "6.1.2",
    "netlify-lambda": "1.4.2",
    "node-sass": "4.11.0",
    "nuxt": "2.4.5",
    "sass-loader": "7.1.0",
    "vue-moment": "4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.15.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-vue": "5.2.2",
    "glob": "7.1.3",
    "prettier": "1.16.1"
  },
  "engines": {
    "node": "10.15.2"
  }
