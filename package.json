{
  "name": "bael-cms-template",
  "version": "0.1.2",
  "description": "A fashionable brutalist blog template using Netlify CMS and Nuxt.js",
  "author": "jake101 <hello@jake101.com>",
  "scripts": {
    "dev": "nuxt",
    "serve": "nuxt",
    "build": "nuxt build",
    "build:api": "netlify-lambda build src/sendgrid",
    "start": "nuxt start",
    "generate": "nuxt generate --quiet --fail-on-error",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "postinstall": "netlify-lambda install",
    "post-update": "yarn upgrade --latest"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.13.1",
    "@nuxtjs/markdownit": "1.2.10",
    "@nuxtjs/pwa": "3.3.5",
    "@sendgrid/client": "7.4.2",
    "bf-solid": "2.11.2",
    "fuse.js": "7.0.0",
    "globby": "^11.0.3",
    "hoek": "6.1.3",
    "netlify-lambda": "^2.0.3",
    "node-sass": "^5.0.0",
    "nuxt": "2.15.4",
    "nuxt-webfontloader": "^1.1.0",
    "sass-loader": "10.1.1",
    "tar": "^6.1.0",
    "vue-moment": "4.1.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.9.0",
    "glob": "7.1.6",
    "postcss-hexrgba": "^2.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-responsive-type": "^1.0.0",
    "prettier": "2.2.1"
  },
  "engines": {
    "node": "10.24.1"
  }
}
